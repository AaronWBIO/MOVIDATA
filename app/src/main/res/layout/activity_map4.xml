<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Map4"
    android:background="@drawable/fondo5">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="336dp"
        android:layout_height="272dp"
        android:layout_marginTop="192dp"
        android:src="@drawable/encuesta1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="96dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:layout_width="50dp"
            android:layout_height="73dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/pesimo"
            android:id="@+id/pesimo"/>

        <ImageView
            android:layout_width="50dp"
            android:layout_height="73dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/malo"
            android:id="@+id/malo"/>

        <ImageView
            android:layout_width="50dp"
            android:layout_height="73dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/normal"
            android:id="@+id/normal"/>

        <ImageView
            android:layout_width="50dp"
            android:layout_height="73dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/bueno"
            android:id="@+id/bueno"/>

        <ImageView
            android:layout_width="58dp"
            android:layout_height="73dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/excelente"
            android:id="@+id/excelente"/>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>